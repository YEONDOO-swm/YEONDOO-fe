{
  "name": "react-admin",
  "private": true,
  "scripts": {
    "dev": "vite --force",
    "build": "vite build",
    "postbuild": "react-snap",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "sitemap": "babel-node src/sitemapGenerator.js",
    "predeploy": "npm run sitemap"
  },
  "reactSnap": {
    "source": "./dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/home",
      "/paperstorage",
      "/history",
      "/userprofile",
      "/paper",
      "/historypaper",
      "/historytrash"
    ]
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.1.2",
    "@coreui/coreui": "^4.2.6",
    "@coreui/react": "^4.9.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.7",
    "@react-oauth/google": "^0.11.1",
    "@sentry/react": "^7.61.0",
    "@sentry/tracing": "^7.61.0",
    "core-js": "^3.32.0",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "^2.0.6",
    "ra-data-json-server": "^4.12.0",
    "react": "^18.2.0",
    "react-admin": "^4.12.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.14.1",
    "react-router-sitemap": "^1.2.0",
    "react-snap": "^1.23.0",
    "styled-components": "^6.0.5",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/register": "^7.22.5",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "msw": "^1.2.2",
    "react-helmet-async": "^1.3.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "homepage": "."
}
